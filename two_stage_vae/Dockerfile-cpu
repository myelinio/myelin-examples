# docker build -t myelinio/two-stage-vae-train-cpu:v0.1.2 -f Dockerfile-cpu .
# docker run --network myelin-trainer  -e TRAIN_CONTROLLER_URL=http://a00240db09ac:5000 myelinio/two-stage-vae-train-cpu:v0.1.0 --budget "6.172839506172839"
FROM tensorflow/tensorflow:1.13.1-py3

RUN pip3 install --upgrade pip

WORKDIR /app

COPY requirements-cpu.txt requirements.txt
RUN for req in $(cat requirements.txt); do pip install $req; done


COPY . /app


ENTRYPOINT ["python", "demo.py"]